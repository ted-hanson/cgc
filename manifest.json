{
  "name": "Crypto-Games Currency Converter",
  "description": "Converts coin amounts to the currency of your choice in the live logs as they stream in",
  "version": "0.3",
  "manifest_version": 2,
  "content_scripts": [
    {
      "run_at" :"document_start",
      "matches": ["https://www.crypto-games.net/*"],
      "js": ["jquery-2.1.0.min.js", "jquery-cookie.js"]
    },
    {
      "run_at" :"document_end",
      "matches": ["https://www.crypto-games.net/*"],
      "js": ["pricer.js"]
    }
  ],
  "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}
